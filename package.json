{
  "name": "@kestra-io/ui-libs",
  "version": "0.0.146",
  "repository": {
    "type": "git",
    "url": "https://github.com/kestra-io/ui-libs"
  },
  "type": "module",
  "files": [
    "dist",
    "src",
    "stub-mdc-imports.js"
  ],
  "main": "./dist/kestra-index.cjs.js",
  "module": "./dist/kestra-index.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/kestra-index.es.js"
      },
      "require": "./dist/kestra-index.cjs.js"
    },
    "./flow-yaml-utils": {
      "types": "./dist/utils/FlowYamlUtils.d.ts",
      "import": {
        "types": "./dist/utils/FlowYamlUtils.d.ts",
        "default": "./dist/kestra-flowyamlutils.es.js"
      },
      "require": "./dist/kestra-flowyamlutils.cjs.js"
    },
    "./vue-flow-utils": {
      "types": "./dist/utils/VueFlowUtils.d.ts",
      "import": {
        "types": "./dist/utils/VueFlowUtils.d.ts",
        "default": "./dist/kestra-vueflowutils.es.js"
      },
      "require": "./dist/kestra-vueflowutils.cjs.js"
    },
    "./style.css": "./dist/ui-libs.css",
    "./src/*": "./src/*"
  },
  "scripts": {
    "dev": "vite",
    "debug": "vite --debug",
    "preview": "vite preview",
    "build": "vite build",
    "build:types": "vue-tsc --project ./tsconfig.build.json",
    "lint": "eslint --fix",
    "test": "vitest",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "watch": "npx concurrently -k -n \"VITE,TYPES\" -c \"magenta,blue\" \"vite build --watch\" \"vue-tsc --watch --project ./tsconfig.build.json\""
  },
  "peerDependencies": {
    "@vue-flow/background": "^1.3.2",
    "@vue-flow/controls": "^1.1.3",
    "@vue-flow/core": "^1.48.2",
    "bootstrap": "^5.3.8",
    "dagre": "^0.8.5",
    "humanize-duration": "^3.33.2",
    "js-yaml": "^4.1.1",
    "lodash": "^4.17.23",
    "moment": "^2.30.1",
    "moment-timezone": "^0.5.48",
    "vue": "^3.5.5",
    "vue-material-design-icons": "^5.3.1",
    "vue-router": "^4.6.4",
    "yaml": "^2.8.2"
  },
  "devDependencies": {
    "@eslint/compat": "^2.0.2",
    "@eslint/js": "^10.0.1",
    "@storybook/addon-docs": "^10.2.12",
    "@storybook/addon-themes": "^10.2.12",
    "@storybook/vue3-vite": "^10.2.12",
    "@types/bootstrap": "^5.2.10",
    "@types/dagre": "^0.7.53",
    "@types/humanize-duration": "^3.27.4",
    "@types/js-yaml": "^4.0.9",
    "@types/lodash": "^4.17.24",
    "@typescript-eslint/parser": "^8.56.1",
    "@vitejs/plugin-vue": "^6.0.4",
    "@vitejs/plugin-vue-jsx": "^5.1.4",
    "@vitest/coverage-v8": "^4.0.18",
    "concurrently": "^9.2.1",
    "eslint": "^10.0.2",
    "eslint-plugin-storybook": "^10.2.12",
    "eslint-plugin-vue": "^10.8.0",
    "globals": "^17.3.0",
    "sass": "^1.97.3",
    "storybook": "^10.2.12",
    "storybook-vue3-router": "^7.0.0",
    "typescript-eslint": "^8.56.1",
    "vite": "^7.3.1",
    "vitest": "^4.0.18",
    "vue-tsc": "^3.2.5"
  },
  "dependencies": {
    "@nuxtjs/mdc": "^0.20.1",
    "@popperjs/core": "^2.11.8",
    "html-to-image": "^1.11.13",
    "mermaid": "^11.12.3",
    "shiki": "^3.23.0",
    "slugify": "^1.6.6",
    "vue-i18n": "^11.2.8"
  },
  "optionalDependencies": {
    "@esbuild/darwin-arm64": "^0.27.3",
    "@esbuild/darwin-x64": "^0.27.3",
    "@esbuild/linux-x64": "^0.27.3",
    "@rollup/rollup-darwin-arm64": "^4.59.0",
    "@rollup/rollup-darwin-x64": "^4.59.0",
    "@rollup/rollup-linux-x64-gnu": "^4.59.0",
    "@swc/core-darwin-arm64": "^1.15.13",
    "@swc/core-darwin-x64": "^1.15.13",
    "@swc/core-linux-x64-gnu": "^1.15.13"
  },
  "overrides": {
    "storybook": "$storybook"
  }
}
